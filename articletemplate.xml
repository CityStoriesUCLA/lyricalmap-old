         </titleStmt>
         <publicationStmt>
            <publisher>Colleen Jaurretche</publisher>
            <date when="2016">[Quarter Year]</date>
         </publicationStmt>
         <notesStmt>
            <relatedItem>
               <bibl>
                  <author></author>
                  <title>[Name of the archival collection], UCLA Library Special Collections</title>
               </bibl>
            </relatedItem>
         </notesStmt>
         <sourceDesc>
            <bibl/>
         </sourceDesc>
